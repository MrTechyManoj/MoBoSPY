            <intent-filter>
                <action android:name="android.service.notification.NotificationListenerService"/>
            </intent-filter>
        </service>
        <receiver android:enabled="true" android:name="com.remote.app.StartActivityOnBootReceiver" android:permission="android.permission.RECEIVE_BOOT_COMPLETED">
